{
  "rewrites": [
    {
      "source": "/doc",
      "destination": "https://docs.privsyn.com"
    },
    {
      "source": "/doc/(.*)",
      "destination": "https://docs.privsyn.com/$1"
    },
    {
      "source": "/docs",
      "destination": "https://docs.privsyn.com"
    },
    {
      "source": "/docs/(.*)",
      "destination": "https://docs.privsyn.com/$1"
    }
  ]
}
